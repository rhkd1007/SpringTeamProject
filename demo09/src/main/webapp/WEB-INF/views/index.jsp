		<thead>
			<tr>
				<th></th>
				<th>제목</th>
				<th>작성자</th>
				<th>작성일</th>
				<th>조회수</th>
			</tr>
		</thead>
		<tbody>
			<c:forEach items="${boards.content}" var="board" varStatus="st">
				<tr>
					<td></td>
					<td><a href="/freeboard/view/${board.num}">(${board.free })${board.title }</a></td>
					<td>${board.member.id }</td>
		
					<td><fmt:formatDate value="${board.regdate}"
							pattern="yyyy-MM-dd" /> <%--  /${board.regdate }  --%></td>
					<td>${board.hitcnt }</td>
				</tr>
			</c:forEach>
		</tbody>
		</table>
        		</div>
        	</div>
        	
